				<width>87</width>
				<height>25</height>
				<font>Font_OSDTime</font>
				<label>$INFO[System.Time]</label>
			</control>
			<control type="label">
				<posx>1260</posx>
				<posy>37</posy>
				<width>87</width>
				<height>25</height>
				<font>Font_OSDTime</font>
				<align>right</align>
				<textcolor>60ffffff</textcolor>
				<label>$LOCALIZE[31710]</label>
			</control>
			<control type="label">
				<posx>1260</posx>
				<posy>60</posy>
				<width>87</width>
				<height>25</height>
				<font>Font_OSDTime</font>
				<align>right</align>
				<label>$INFO[Player.FinishTime]</label>	
			</control>		
		</control>
		<control type="group">
			<animation type="Visible">
				<effect type="fade" time="400" start="0" end="100" />
				<effect type="slide" time="400" start="0,-151" end="0,0" easing="out" tween="cubic" />
			</animation>
			<animation type="Hidden">
				<effect type="fade" time="400" start="100" end="0" />
				<effect type="slide" time="400" start="0,0" end="0,-151" easing="in" tween="cubic" />
			</animation>
			<animation type="WindowOpen">
				<effect type="fade" time="400" start="0" end="100" />
				<effect type="slide" time="400" start="0,-151" end="0,0" easing="out" tween="cubic" />
			</animation>
			<animation type="WindowClose">
				<effect type="fade" time="400" start="100" end="0" />
				<effect type="slide" time="400" start="0,0" end="0,-151" easing="in" tween="cubic" />
			</animation>
			<description>top controls</description> 	
			<control type="image">
				<description>Panel</description>
				<posx>0</posx>
				<posy>-620</posy>
				<height>80</height>
				<aligny>top</aligny>
				<width>1280</width>
				<texture>osd/codec_top.png</texture>
			</control>
			<control type="label">
